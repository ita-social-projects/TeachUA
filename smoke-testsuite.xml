<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test suite of basic functionality">
    <parameter name="mode" value="headless"/>
    <test name="Links and images validation" enabled="true">
        <classes>
            <class name="com.softserve.edu.test.ui.BaseTest">
                <methods>
                    <include name="linksImagesTest" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Basic club search" enabled="true">
        <classes>
            <class name="com.softserve.edu.test.ui.clubs.SearchClubTest">
                <methods>
                    <include name="basicClubSearchTest" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="Advanced club search" enabled="true">
        <classes>
            <class name="com.softserve.edu.test.ui.clubs.SearchClubTest">
                <methods>
                    <include name="advancedClubSearchTest" />
                </methods>
            </class>
        </classes>
    </test>
</suite>